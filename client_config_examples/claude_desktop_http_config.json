{
  "mcpServers": {
    "tomtom-route-remote-v1": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://192.168.1.3:8081/mcp",
        "--allow-http"
      ]
    },
    "tomtom-route-remote-v2": {
      "connector": {
        "type": "http",
        "url": "http://192.168.1.3:8081/mcp"
      },
      "options": {
        "allow-http": true
      }
    },
    "tomtom-route-remote-v3": {
      "transport": {
        "type": "http",
        "host": "192.168.1.3",
        "port": 8081,
        "path": "/mcp"
      }
    }
  }
}
